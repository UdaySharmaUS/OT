clc
clear all
C = [2, 3,4,7]
A=[2 3 -1 4; 1 2 6 -7]
B=[8; -3]

n = size(A, 2)
m = size(A, 1)
if n>m
    nCm = nchoosek(n, m)
    p = nchoosek(1:n, m)
    sol = []
    for i = 1: nCm
        y = zeros(n, 1);
        A1 = A(: , p(i, :));
        X = inv(A1) * B;
        if all(X>=0 & X~=inf & X~=inf)
            y(p(i, :)) = X
            sol = [sol y]
        end
    end
else
    error('No of variables are not greater than no of equations')
end
z = C*sol
